//:::::::::::::::::::: Raleway ::::::::::::::::::::::::

@font-face {
  font-family: 'Raleway';
  src: font-url('raleway/raleway-bold-webfont.eot');
  src: font-url('raleway/raleway-bold-webfont.eot?#iefix') format('embedded-opentype'),
  font-url('raleway/raleway-bold-webfont.woff2') format('woff2'),
  font-url('raleway/raleway-bold-webfont.woff') format('woff'),
  font-url('raleway/raleway-bold-webfont.ttf') format('truetype'),
  font-url('raleway/raleway-bold-webfont.svg#ralewaybold') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'Fira';
  src: font-url('fira/firasans-semibold-webfont.eot');
  src: font-url('fira/firasans-semibold-webfont.eot?#iefix') format('embedded-opentype'),
  font-url('fira/firasans-semibold-webfont.woff') format('woff'),
  font-url('fira/firasans-semibold-webfont.ttf') format('truetype'),
  font-url('fira/firasans-semibold-webfont.svg#fira_sanssemibold') format('svg');
  font-weight: normal;
  font-style: normal;
}